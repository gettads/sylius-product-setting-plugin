{
    "name": "gtt/sylius-product-setting-plugin",
    "type": "sylius-plugin",
    "description": "ProductSetting plugin for Sylius.",
    "keywords": [
        "sylius",
        "sylius-plugin",
        "product-setting"
    ],
    "license": "proprietary",
    "require": {
        "php": "^8.1",
        "phpdocumentor/reflection-docblock": "^5.3",
        "sylius/sylius": "^1.12",
        "symfony/http-client": "^6.2"
    },
    "require-dev": {
        "api-platform/core": ">=2.7",
        "behat/behat": "^3.6.1",
        "behat/mink-selenium2-driver": "^1.4",
        "dama/doctrine-test-bundle": "^7.2",
        "dmore/behat-chrome-extension": "^1.3",
        "dmore/chrome-mink-driver": "^2.7",
        "doctrine/collections": ">=1.8",
        "doctrine/data-fixtures": "^1.6",
        "doctrine/dbal": ">=3.6",
        "friends-of-behat/mink": "^1.10",
        "friends-of-behat/mink-browserkit-driver": "^1.4",
        "friends-of-behat/mink-extension": "^2.4",
        "friends-of-behat/page-object-extension": "^0.3",
        "friends-of-behat/suite-settings-extension": "^1.0",
        "friends-of-behat/symfony-extension": "^2.1",
        "friends-of-behat/variadic-extension": "^1.3",
        "friendsofsymfony/rest-bundle": ">=3.1.0",
        "phpstan/phpstan": "^1.10",
        "polishsymfonycommunity/symfony-mocker-container": "^1.0.6",
        "slevomat/coding-standard": "^8.3",
        "squizlabs/php_codesniffer": "^3.7",
        "symfony/browser-kit": "^4.4|^5.2",
        "symfony/debug-bundle": "^4.4|^5.2|^6.0",
        "symfony/dom-crawler": ">=6.0",
        "symfony/dotenv": "^6.2",
        "symfony/error-handler": "^6.2",
        "symfony/css-selector": "^6.0",
        "symfony/test-pack": "^1.1",
        "symfony/web-link": ">=5.3.0",
        "symfony/web-profiler-bundle": "^6.0",
        "twig/twig": "> 3.4.0",
        "webmozart/assert": ">=1.11"
    },
    "conflict": {
        "babdev/pagerfanta-bundle": "<3.6.1",
        "jms/serializer-bundle": "<4.2.0",
        "knplabs/knp-menu": "<3.3.0",
        "lexik/jwt-authentication-bundle": "<2.12.0|v2.14.0",
        "symfony/security-bundle": "<6.2",
        "willdurand/negotiation": "<3.0.0"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.10-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Gtt\\SyliusProductSettingPlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Gtt\\SyliusProductSettingPlugin\\Unit\\": "tests/Unit",
            "Tests\\Gtt\\SyliusProductSettingPlugin\\Behat\\": "tests/Behat/",
            "App\\": "tests/Application/src/"
        },
        "classmap": [
            "tests/Application/Kernel.php"
        ]
    },
    "scripts": {
        "analyze": [
            "vendor/bin/phpstan analyse -c phpstan.neon",
            "vendor/bin/phpcs"
        ],
        "tests": [
            "vendor/bin/phpunit",
            "vendor/bin/behat"
        ],
        "check": [
            "@analyze",
            "@tests"
        ],
        "post-install-cmd": [
            "php bin/create_node_symlink.php"
        ],
        "post-update-cmd": [
            "php bin/create_node_symlink.php"
        ],
        "post-create-project-cmd": [
            "php bin/create_node_symlink.php"
        ]
    }
}
